    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.23.0",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^3.16.1",
    "express": "^4.14.1",
    "hbs": "^4.0.1",
    "mocha": "^3.2.0",
    "rollup": "^0.41.4",
